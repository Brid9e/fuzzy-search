{
  "name": "@brid9e/fuzzy-search",
  "version": "0.0.1-alpha.2",
  "type": "module",
  "description": "智能模糊搜索库 - 性能优化版TypeScript实现",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tsdown",
    "dev": "pnpm exec tsdown --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "fuzzy",
    "search",
    "typescript",
    "chinese",
    "performance",
    "similarity",
    "matching"
  ],
  "author": {
    "name": "@brid9e",
    "url": "https://github.com/Brid9e"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brid9e/fuzzy-search.git"
  },
  "bugs": {
    "url": "https://github.com/Brid9e/fuzzy-search/issues"
  },
  "homepage": "https://github.com/Brid9e/fuzzy-search#readme",
  "devDependencies": {
    "tsdown": "^0.2.17",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.0.0"
}
